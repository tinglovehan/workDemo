.page-line {
    height: 22rpx;
    background-color: #f4f4f4;
    border-top: 1px solid #ddd;
    clear: both;
}
.page-line2 {
    height: 22rpx;
    background-color: #f4f4f4;
    border-bottom: 1px solid #dcdcdc;
    clear: both;
}
.tab-li {
    float: left;
    box-sizing: border-box;
}
.tab {
    background-color: #fff;
    border-bottom: 1px solid #dcdcdc;
}
.c-base,.cf90{
  color: #f90 !important;
}
.tab-li-a {
    display: block;
    text-align: center;
    font-size: 28rpx;
}
.order-list {
    background-color: #fff;
}
.order-list-li {
    border-bottom: 1px solid #dcdcdc;
    padding: 10rpx 20rpx;
    font-size: 28rpx;
    color: #999;
    overflow: hidden;
    position: relative;
   
}
.myorder-list-li, .account-list-li {
    display: table;
    width: 100%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.lab-title {
    font-size: 28rpx;
    color: #333;
    float: left;
    padding-right:10rpx;
    position: relative;
    vertical-align: middle;
}
.myorder-list .lab-title, .account-list .lab-title {
    width: 160rpx;
    display: table-cell;
    vertical-align: middle;
    float: none;
    overflow: hidden;
}
.order-item {
    position: relative;
    min-height: 50rpx;
    height: auto;
    line-height: 50rpx;
    margin-top: 15rpx;
    z-index: 3;
  
}
.c333{
  color: #333;
}
.price, .original-price {
    display: inline-block;
    vertical-align: middle;
    font-size: 26rpx;
    color: #ccc;
}
.price-em {
    color: #f90;
    font-size: 28rpx;
    margin-right: 5rpx;
}
.original-price {
    text-decoration: line-through;
}
.numSpinnerBox input {
    width:70rpx;
    line-height: 44rpx;
    text-align: center;
    border: none;
    vertical-align: middle;
    box-sizing: border-box;
    font-size: 28rpx;
    
    margin: 0 5rpx;
    color: #f90;
}
.realNameTip {
    height: 40rpx;
    line-height: 40rpx;
    color: #fff;
    background-color: #f63;
    text-align: center;
    margin: 0 5rpx;
    font-size: 28rpx;
    padding: 5rpx 10rpx;
    border-radius: 3px;
    overflow: hidden;
}
.total-price {
    font-size: 30rpx;
    color: #333;
    padding-left: 20rpx;
}
.btn {
    display: block;
    margin: 20rpx 30rpx;
    text-align: center;
    height: 82rpx;
    line-height: 82rpx;
    color: #fff;
    font-size:32rpx;
    border-radius: 5px;
    background-color: #f90;
    border: none;
}
.btn-RP {
    display: block;
    text-align: center;
    height: 82rpx;
    line-height: 82rpx;
    margin: 30rpx;
    color: #666;
    font-size:32rpx;
    border-radius: 5px;
    border: 1px solid #dcdcdc;
    background-color: #FFF;
}
.nodata{
  color: #f60;
  text-align: center;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 28rpx;
}
.dvMsgBox {
     position: fixed;
    font-size:28rpx;
    width: 600rpx;
    overflow: hidden;
    z-index: 109999;
    border-radius: 3px;
    display: block;
    
    top: 35%;
  
    left: 50%;
    margin-left: -300rpx;
}
.dvMsgBox .top {
    height: 80rpx;
    background-color: #f90;
    padding-left: 32rpx;
    
    width: 100%;
    color: #fff;
    line-height: 80rpx;
}
.dvMsgBox .body {
    background: #fff;
     width: 100%;
}
.dvMsgBox .ct {
    vertical-align: middle;
    line-height: 160%;
    text-align: left;
    color: #333;
    padding: 20rpx;
    font-size: 28rpx;
}
.dvMsgBox .ct-p{
  display: block;
  text-align: left;
  padding-top: 20rpx;

}
.dvMsgBox .bottom {
    background: #fff;
    padding-left: 12rpx;
    float: left;
    width: 100%;
    padding-bottom: 20rpx;
}
.dvMsgBtns{
  text-align: center;
    width: 100%;
    margin: 0;
    line-height: 1;
    height: auto;
    font-size: 30rpx;
    background: none;
}
.bgf60{
  background: #f60 !important;
  color: #fff !important;
}