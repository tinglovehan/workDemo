    height: 84rpx;
}
.navList {
    width: 100%;
    font-size: 26rpx;
    height: 84rpx;
    overflow: hidden;
    padding: 0 10rpx;
}
.navList-li {
    display: inline-block;
    vertical-align: middle;
   height: 80rpx;
   line-height: 80rpx;
    width: auto;
    font-size: 28rpx;
    text-align: center;
    position: relative;
    margin: 0 20rpx;
    padding: 0 10rpx;
    color: #666;
   
}
.navList-li .icon-hot{ font-size: 48rpx; margin-right: 5rpx; float: left; margin-top: 5rpx;}
.navList-li.on{ color: #ff9900; border-bottom: 2px solid #ff9900;}
.couponCenter {
    position: relative;
    z-index: 2;
    width: 25%;
    font-size: 28rpx;
    line-height:84rpx;
    height: 84rpx;
    text-align: center;
    color: #cdcdcd;
}
.swiper-container{
    position: relative;
    text-align: center;
    line-height: 0px;
    margin-right: auto;
    margin-left: auto;

}
.swiper-container .swiper_box{
    overflow: hidden;
    position: relative;
    height: 375rpx;
}
.swiper-container .swiper_box .swiper_item{
  width: 100%;
  height: 375rpx;
}
.swiper_box .swiper_item .slide-image{
  display: block;
  width: 100%;
  height: 375rpx;
}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 60rpx;  
  height: 8rpx;  
  background: #fff;  
 transition: all .6s;  
  opacity: .5;
  
}  
.swiper-container .dots .active{  
  opacity: 1;
  background: #ff9900 !important;
}
