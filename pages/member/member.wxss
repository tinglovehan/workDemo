.member-header {
    height: 300rpx;
    background: #eee;
    width: 100%;
    text-align: center;
    padding-top: 60rpx;
    background: url("http://b2c-img-jingqu.sendinfo.com.cn/group1/M00/00/01/wKjIRFpO0eSAeRseAADMnfkd4oc790.JPG") no-repeat;
    background-size: cover;
    }

.member-header  .icon{
    font-size: 80rpx;
    color: #f60;
    padding-top: 40rpx;
    display: block;
    
    }
.memberName{
      font-size: 30rpx;
      padding-top: 20rpx;
      color: #fff;
 }
 .member-tab {
    padding: 20rpx 0;
}
.member-tab-li {
    width: 25%;
    position: relative;
    float: left;
    border-right: 1px solid #e5e5e5;
    box-sizing: border-box;
}
.member-tab-a {
    display: block;
    text-align: center;
    font-size: 28rpx;
    color: #999;
}

.dingdan-pay,.dingdan-complent {
    color: #3c9;
}
.member-tab-i {
    display: block;
    margin: 0 auto 5rpx;
    font-size: 46rpx;
}
.dingdan-unpay,.dingdan-refund{
    color: #f66;
}
.member-tab-li:last-child {
    border-right: none;
}
.order-list-li-a{
      display: block;
    color: #333;
    overflow: hidden;
}
.lab-title-i {
    margin-right: 10rpx;
    font-size:44rpx;
    vertical-align: middle;
    display: inline-block;
    height: 80rpx;
    line-height: 75rpx;
}
.payed{
  color: #693;
}
.wait_pay{
  color: #c33;
}
.finish{
  color: #f90;
}
.refund{
color: #00a8ff;
}
.member-list-li{
  padding: 32rpx 20rpx;
}
.member-list-li .font-icon{
  color: #999;
}
.user-face {
    width: 104rpx;
    height: 104rpx;
    display: block;
    margin: 0 auto;
    border-radius: 100%;
    background-color: rgba(255, 255, 255, 0.6);
    text-align: center;
    line-height: 104rpx;
    color: #fff;
    overflow: hidden;
}
.user-face image {
    width: 100%;
    height: 100%;
}